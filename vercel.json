{
  "version": 2,
  "buildCommand": "npm run build",
  "cleanUrls": true,
  "public": false,
  "build": {
    "env": {
      "VERCEL_FORCE_NO_BUILD_CACHE": "1"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "preview": false
    }
  },
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true,
    "productionBranch": "main"
  }
}
